package com;

public class Calculator {
 public int add(int a, int b) {
  return a+b;
 }
 public int sub(int a, int b) {
	  return a-b;
	 }
 public int mul(int a, int b) {
	  return a*b;
	 }
 public int dic(int a, int b) {
	  try{return a/b;}
	  catch(ArithmeticException e) {
		  System.out.println("Can't be divided by Zero"+e);
	  }
	 }
}
